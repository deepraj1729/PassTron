{
  "name": "passtron",
  "version": "0.1.0",
  "description": "Password Manager Desktop Application",
  "author": {
    "name": "Deepraj Baidya",
    "email": "bdeeprajrkm1@gmail.com",
    "url": "https://github.com/deepraj1729/"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . passtron",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PassTron Security PassWord Manager\"",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PassTron Security PassWord Manager\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PassTron Security PassWord Manager\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepraj1729/passtron.git"
  },
  "homepage": "https://github.com/deepraj1729/passtron#readme",
  "bugs": {
    "url": "https://github.com/deepraj1729/passtron/issues"
  },
  "keywords": [
    "app",
    "application",
    "desktop",
    "desktop-app",
    "desktop-application",
    "electron",
    "electron-app",
    "password-manager",
    "javascript"
  ],
  "dependencies": {
    "electron": "^9.1.1",
    "electron-packager": "^9.1.0"
  },
  "devDependencies": {}
}
